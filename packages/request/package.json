{
  "name": "@org/request",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "sourceRoot": "packages/request/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/request/dist",
          "main": "packages/request/src/index.ts",
          "tsConfig": "packages/request/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@org/ts-utils": "workspace:^",
    "@swc/helpers": "~0.5.11",
    "@types/lodash.merge": "^4.6.9",
    "axios": "^1.7.9",
    "lodash.merge": "^4.6.2"
  }
}
